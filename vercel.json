{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NEON_DATABASE_URL": "@neon_database_url",
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_pass",
    "SMTP_FROM": "@smtp_from",
    "ORDER_NOTIFICATION_EMAIL": "@order_notification_email",
    "ADMIN_TOKEN": "@admin_token"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
